{
    "manifest_version": 2,
    "name": "Google extensions test",
    "description": "Show current page URL",
    "version": "1.0",
    "browser_action": {
        "default_icon": "assets/icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs", "<all_urls>"
    ],
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["script.js"]
    }]
}